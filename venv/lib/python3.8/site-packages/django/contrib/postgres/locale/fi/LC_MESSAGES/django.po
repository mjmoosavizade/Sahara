#: venv/lib/python3.8/site-packages/django/contrib/postgres/apps.py:42
msgid "PostgreSQL extensions"
msgstr "PostgreSQL-laajennukset"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:21
#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:16
#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:170
#, fuzzy, python-format
#| msgid "Item %(nth)s in the array did not validate: "
msgid "Item %(nth)s in the array did not validate:"
msgstr "Taulukon %(nth)s alkio ei ole kelvollinen:"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Sisäkkäisillä taulukoilla tulee olla sama pituus."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:16
msgid "Map of strings to strings/nulls"
msgstr "Kartta merkkijonoista merkkijonoihin tai tyhjiin (null)"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:18
#, fuzzy, python-format
#| msgid "The value of \"%(key)s\" is not a string or null."
msgid "The value of “%(key)s” is not a string or null."
msgstr "Avaimen \"%(key)s\" arvo ei ole merkkijono tai tyhjä (null)."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/jsonb.py:32
msgid "A JSON object"
msgstr "JSON-tietue"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/jsonb.py:34
msgid "Value must be valid JSON."
msgstr "Arvon pitää olla kelvollista JSONia."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:16
msgid "Could not load JSON data."
msgstr "JSON-dataa ei voitu ladata."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "Syötteen tulee olla JSON-sanakirja."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/jsonb.py:19
#, fuzzy, python-format
#| msgid "'%(value)s' value must be valid JSON."
msgid "“%(value)s” value must be valid JSON."
msgstr "%(value)s-arvo tulee olla kelvollista JSONia."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:37
msgid "Enter two valid values."
msgstr "Anna kaksi oikeaa arvoa."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:38
msgid "The start of the range must not exceed the end of the range."
msgstr "Alueen alku pitää olla pienempi kuin alueen loppu."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:86
msgid "Enter two whole numbers."
msgstr "Anna kaksi kokonaislukua."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:92
msgid "Enter two numbers."
msgstr "Anna kaksi lukua."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:109
msgid "Enter two valid date/times."
msgstr "Anna kaksi oikeaa päivämäärää/kellonaikaa."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:115
msgid "Enter two valid dates."
msgstr "Anna kaksi oikeaa päivämäärää."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Listassa on %(show_value)d arvo, mutta siinä ei saa olla enempää kuin "
"%(limit_value)d."
msgstr[1] ""
"Listassa on %(show_value)d arvoa, mutta siinä ei saa olla enempää kuin "
"%(limit_value)d."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Listassa on %(show_value)d arvo, mutta siinä ei saa olla vähempää kuin "
"%(limit_value)d arvoa."
msgstr[1] ""
"Listassa on %(show_value)d arvoa, mutta siinä ei saa olla vähempää kuin "
"%(limit_value)d."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Joitain avaimia puuttuu: %(keys)s"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Tuntemattomia avaimia annettiin: %(keys)s"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr ""
"Tämän alueen tulee olla kokonaisuudessaan yhtäsuuri tai pienempi kuin "
"%(limit_value)s."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr ""
"Tämän alueen tulee olla kokonaisuudessaan yhtäsuuri tai suurempi kuin "
"%(limit_value)s."
